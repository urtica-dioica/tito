        /* istanbul ignore next */
        (cov_7esvhu1wu().b[24][1]++, `Clocked out at ${new Date(row.clockOut).toLocaleTimeString()}`),
        timestamp: row.createdAt,
        status: 'success'
      };
    }));
    /* istanbul ignore next */
    cov_7esvhu1wu().s[88]++;
    return activities;
  }
