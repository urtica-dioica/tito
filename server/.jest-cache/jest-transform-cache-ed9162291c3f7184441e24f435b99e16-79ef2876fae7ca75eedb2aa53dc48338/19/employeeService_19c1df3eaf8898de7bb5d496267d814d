          cov_7esvhu1wu().s[236]++;
          employeeData.push([benefit.name, `+${benefit.amount}`]);
        });
        /* istanbul ignore next */
        cov_7esvhu1wu().s[237]++;
        employeeData.push(['', '']); // Add empty row
      } else
      /* istanbul ignore next */
      {
        cov_7esvhu1wu().b[62][1]++;
      }
      // Add deductions section
      cov_7esvhu1wu().s[238]++;
      employeeData.push(['Deductions Breakdown', '']);
      /* istanbul ignore next */
      cov_7esvhu1wu().s[239]++;
