  /**
   * Get latest employee paystub
   */
  async getLatestPaystub(employeeId) {
    /* istanbul ignore next */
    cov_7esvhu1wu().f[41]++;
    // First try to get the latest payroll record
    const query =
    /* istanbul ignore next */
    (cov_7esvhu1wu().s[313]++, `
      SELECT 
        pr.id,
        pr.payroll_period_id,
        pp.period_name,
