      cov_7esvhu1wu().s[96]++;
      throw new Error('Employee not found');
    } else
    /* istanbul ignore next */
    {
      cov_7esvhu1wu().b[26][1]++;
    }
    const row =
    /* istanbul ignore next */
    (cov_7esvhu1wu().s[97]++, result.rows[0]);
