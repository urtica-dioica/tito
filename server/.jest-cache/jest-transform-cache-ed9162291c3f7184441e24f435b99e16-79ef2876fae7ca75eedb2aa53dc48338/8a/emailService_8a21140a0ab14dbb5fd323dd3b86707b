      port: environment_1.config.email.port,
      secure: environment_1.config.email.port === 465,
      // true for 465, false for other ports
      auth: {
        user: environment_1.config.email.user,
        pass: environment_1.config.email.pass
      }
    });
  }
  /**
   * Send email
   */
  async sendEmail(options) {
    /* istanbul ignore next */
    cov_2qp2tnm67l().f[2]++;
    cov_2qp2tnm67l().s[8]++;
    try {
      const mailOptions =
      /* istanbul ignore next */
      (cov_2qp2tnm67l().s[9]++, {
        from: environment_1.config.email.from,
        to: options.to,
        subject: options.subject,
        html: options.html,
        text: options.text
      });
      const result =
      /* istanbul ignore next */
      (cov_2qp2tnm67l().s[10]++, await this.transporter.sendMail(mailOptions));
      /* istanbul ignore next */
      cov_2qp2tnm67l().s[11]++;
      logger_1.default.info('Email sent successfully', {
        messageId: result.messageId,
        to: options.to,
        subject: options.subject
      });
      /* istanbul ignore next */
      cov_2qp2tnm67l().s[12]++;
      return true;
    } catch (error) {
      /* istanbul ignore next */
      cov_2qp2tnm67l().s[13]++;
      logger_1.default.error('Failed to send email', {
        error: error.message,
        to: options.to,
        subject: options.subject
      });
      /* istanbul ignore next */
      cov_2qp2tnm67l().s[14]++;
      return false;
    }
  }
